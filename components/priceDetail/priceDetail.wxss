.price_content {
    padding: 16rpx;
    box-sizing: border-box;
}

.price_item {
    height: 80rpx;
    line-height: 80rpx;
}
.overflow_auto {
    overflow: auto;
    -webkit-overflow-scrolling: touch;
  }
.price_icon {
    display: flex;
    height: 100%;
    align-items: center;
}
.clickColor {
    background: rgba(246, 246, 246, 0.6);
}
.clearFix:after {
    content: '';
    display: block;
    width: 0;
    height: 0;
    clear: both;
}

.left {
    float: left;
}
.right {
    float: right;
}
.modal_container {
    box-sizing: border-box;
    font-weight: 400;
    color: rgba(67, 67, 67, 1);
    position: relative;
}

.modal_title {
    font-size: 32rpx;
    color: rgba(67, 67, 67, 1);
    height: 96rpx;
    line-height: 96rpx;
    background-color: white;
    position: sticky;
    top: 0;
}
.modal_bold {
    font-weight: 500;
}
.goods_margin_bottom {
    margin-bottom: 40rpx;
}
.padding {
    padding: 0 32rpx;
}
.prices {
    font-weight: 400;
    color: rgba(153, 153, 153, 1);

}
.border_bottom {
    border-bottom: 1px solid #f5f5f5;
  }
.portDeliveryFee {
    padding-bottom: 24rpx;
    border-bottom: 1px solid #f5f5f5;
}

.prices_title {
    font-weight: 500;
    color: rgba(67, 67, 67, 1);
}

.prices_item {
    padding: 24rpx 0;
}
  