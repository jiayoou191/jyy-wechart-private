.userInfo {
    background: rgba(242, 242, 244, 1);
    height: 100%;
    padding: 32rpx 32rpx 112rpx 32rpx;
    box-sizing: border-box;
    overflow: scroll;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(67, 67, 67, 1);
}

.userInfo_title {
    height: 192rpx;
    padding: 32rpx;
    box-sizing: border-box;
    background: rgba(255, 255, 255, 1);
    border-radius: 16rpx;
    margin-bottom: 32rpx;
}

.userPhoto {
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    margin-right: 16rpx;
}

.nickName {
    font-size: 32rpx;
    font-weight: 500;
    color: rgba(67, 67, 67, 1);
}

.userInfo_order {
    height: 80rpx;
    padding-right: 32rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: rgba(255, 255, 255, 1);
    border-radius: 16rpx;
    margin-bottom: 32rpx;
}

.text_center {
    text-align: center;
    height: 100%;
    line-height: 100%;
}

.label_icon {
    width: 24rpx;
    height: 28rpx;
}

.userInfo_menu {
    background: rgba(255, 255, 255, 1);
    border-radius: 16rpx;
}

.menu_item {
    width: 100%;
    height: 80rpx;
    position: relative;
}

.menu_icon_parent {
    width: 72rpx;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.menu_icon {
    width: 40rpx;
    height: 40rpx;
}

.border_bottom {
    border-bottom: 1px solid #f5f5f5;
}

.padding {
    height: 100%;
    padding-right: 32rpx;
    box-sizing: border-box;
}

.version {
    font-weight: 400;
    color: rgba(67, 67, 67, 1);
    font-size: 28rpx;
}

.clickColor {
    background: rgba(246, 246, 246, 0.6);
}

.contact {
    position: absolute;
    border: 0;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: left;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(67, 67, 67, 1);
    padding: 0 0 0 72rpx;
    left: 0;
    background-color: transparent;
    z-index: 99;
}

.contact::after {
    border: 0;
}

.contact_icon {
    position: absolute;
    right: 32rpx;
}
.pop_height{
    /* display: none; */
    opacity: 0;
   height: 120rpx;
   margin-top: 120rpx;
}