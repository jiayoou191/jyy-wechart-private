.review {
    background: rgba(242, 242, 244, 1);
    /* height: 100%;
    overflow: hidden; */
    padding: 32rpx 32rpx 112rpx 32rpx;
    box-sizing: border-box;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(67, 67, 67, 1);
}

.clickColor {
    background: rgba(246, 246, 246, 0.6);
    /* background-color: rgba(242, 242, 244, 1); */
}

.goods,
.plan_info {
    padding: 16rpx;
    box-sizing: border-box;
    font-size: 24rpx;
}

.goods_items {
    font-size: 28rpx;
    font-weight: 500;
    color: rgba(67, 67, 67, 1)
}

.goods_title,
.goods_item {
    padding: 24rpx 0;
}

.goods_name,
.goods_weight {
    margin-right: 30rpx;
}
.goods_name{
    /* display: inline-block; */
    max-width: 200rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.light_color {
    font-weight: 400;
    color: rgba(170, 170, 170, 1);
}

.boxes_plan {
    padding: 16rpx;
    height: 60rpx;
    background: rgba(242, 242, 244, 1);
    border-radius: 8rpx;
    box-sizing: border-box;
    position: relative;
}

.boxes_plan_label {
    position: absolute;
    top: 50%;
    left: 16rpx;
    transform: translateY(-50%);
}

.box_item {
    height: 32rpx;
    position: absolute;
    top: 50%;
    left: 130rpx;
    transform: translateY(-50%);
}

.box_item_space {
    margin-right: 16rpx;
}

.margin_right_8 {
    margin-right: 16rpx;
}

.circular {
    width: 12rpx;
    height: 12rpx;
    border-radius: 50%;
    margin-right: 12rpx;
}

.circular_green {
    background: rgba(20, 168, 40, 1);
}

.circular_red {
    background: rgba(220, 62, 56, 1);
}

.plan_info_content {
    padding: 16rpx 0 16rpx 24rpx;
}

.pol,
.pod {
    color: #999;
}

.supplier {
    background: rgba(242, 242, 244, 1);
    border-radius: 12rpx;
    height: 60rpx;
    line-height: 30rpx;
    padding: 16rpx;
    box-sizing: border-box;
    margin: 16rpx 0;
}

.remark_icon {
    width: 28rpx;
    height: 28rpx;
    margin-right: 8rpx;
    margin-top: 4rpx;
}

.remarks {
    max-height: 68rpx;
    line-height: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    color: #AAAAAAFF;
}

.trade_terms {
    height: 80rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 16rpx;
    padding: 16rpx;
    line-height: 48rpx;
    box-sizing: border-box;
    color: #AAAAAAFF;
    margin-bottom: 30rpx;
}

.trade_terms_title {
    font-weight: 500;
    color: rgba(67, 67, 67, 1);
}

.tradeTerm_item {
    font-size: 28rpx;
    padding: 32rpx 0;
    border-bottom: 1px solid #f5f5f5;
    color: rgba(67, 67, 67, 1);
    position: relative;
}

.tradeTerm_label {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(170, 170, 170, 1);
}

.price_content {
    padding: 16rpx;
    box-sizing: border-box;
}

.price_item {
    height: 80rpx;
    line-height: 80rpx;
}

.price_icon {
    display: flex;
    height: 100%;
    align-items: center;
}

.modal_container {
    box-sizing: border-box;
    font-weight: 400;
    color: rgba(67, 67, 67, 1);
    position: relative;
}

.padding {
    padding: 0 32rpx;
}

.modal_title {
    font-size: 32rpx;
    color: rgba(67, 67, 67, 1);
    height: 96rpx;
    line-height: 96rpx;
    background-color: white;
    position: sticky;
    top: 0;
    z-index: 9;
}

.modal_bold {
    font-weight: 500;
}

.goods_items {
    margin-bottom: 48rpx;
}

.goods_null {
    height: 200rpx;
    line-height: 100rpx;
    text-align: center;
}

.goods_margin_bottom {
    margin-bottom: 40rpx;
}

.tradeTerms_modal {
    width: 200%;
    padding-bottom: 32rpx;
    transition: all 0.5s;
    max-height: 972rpx;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.tradeTerms_modal_sliper_left {
    transform: translateX(-50%);
}

.tradeTerms,
.tradeTerms_price {
    width: 50%;
    padding-right: 32rpx;
    box-sizing: border-box;
    position: relative;
}

.tradeTerm_item_checked {
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%);
}

.tradeTerms_back_icon {
    width: 72rpx;
    margin-left: -16rpx;
}

.tradeTerms_price {
    font-weight: 400;
    color: rgba(170, 170, 170, 1);
}

.price_confirm {
    font-size: 28rpx;
    font-weight: 500;
    color: rgba(39, 60, 225, 1);
    padding: 0 32rpx;
    margin-right: -32rpx;
}

.tradeTerms_prices_item {
    margin-bottom: 52rpx;
}

.tradeTerms_price_title {
    padding-bottom: 12rpx;
    border-bottom: 1px solid #f5f5f5;
}

.tradeTerms_price_item {
    padding-top: 28rpx;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #f5f5f5;
}

.input_label {
    width: 176rpx;
    font-weight: 400;
    color: rgba(67, 67, 67, 1);
    height: 32rpx;
    line-height: 32rpx;
}

.prices {
    font-weight: 400;
    color: rgba(153, 153, 153, 1);

}

.portDeliveryFee {
    padding-bottom: 24rpx;
    border-bottom: 1px solid #f5f5f5;
}

.prices_title {
    font-weight: 500;
    color: rgba(67, 67, 67, 1);
}

.prices_item {
    padding: 24rpx 0;
}

.sellerFee_modal {
    padding: 32rpx 32rpx 144rpx 32rpx;
}

.sellerFee {
    padding: 32rpx 0;
    border-bottom: 1px solid #f5f5f5;
}

.priceSubmit {
    width: 100%;
    height: 112rpx;
}

.sys_scroll {
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
}

.percent {
    width: 200rpx;
    text-align: left;
    padding-left: 32rpx;
}

.goods_volume {
    width: 200rpx;
    text-align: center;
}
.max_width_3{
    max-width: 532rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.max_width_5{
    max-width: 552rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.max_width{
    max-width: 520rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.mar_right{
    margin-right: 10rpx;
}
 